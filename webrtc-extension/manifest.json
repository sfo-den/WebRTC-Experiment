{
	"name" : "ScreenBroadcast",
	"version" : "1.0",
	"manifest_version" : 2,
	"description" : "Screen Broadcasting using WebRTC! - Muaz Khan",
	"background": {
		"scripts": ["broadcaster.js"],
		"persistent": false
	},
	"browser_action" : {
		"default_icon" : "images/tabCapture22.png",
		"default_title" : "Broadcast your Screen!"
	},
	"icons" : {
		"16" : "images/tabCapture16.png",
		"22" : "images/tabCapture22.png",
		"32" : "images/tabCapture32.png",
		"48" : "images/tabCapture48.png",
		"128": "images/tabCapture128.png"
	},
	"permissions": [
		"tabCapture"
	]
  }
